{
    "manifest_version": 2,
    "name": "uAlias",
    "version": "1.0",

    "description": "Associate short strings, aliases, with URLs",

    "permissions": [
        "storage",
        "tabs"
    ],

    "omnibox": {
        "keyword": "u"
    },

    "background": {
        "scripts": ["background.js"]
    },

    "options_ui": {
        "page": "options.html"
    }
}
